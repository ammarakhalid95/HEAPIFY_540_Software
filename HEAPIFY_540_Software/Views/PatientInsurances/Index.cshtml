@model IEnumerable<HEAPIFY_540_Software.Models.PatientInsurance>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Patient
        </th>
        <th>
           Insurance
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
             @Html.DisplayFor(modelItem => item.Patient.FullName)
            
        </td>
        <td>
           @Html.DisplayFor(modelItem => item.Insurance.InsuranceName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PatientInsuranceID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PatientInsuranceID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PatientInsuranceID })
        </td>
    </tr>
}

</table>
